{
  "name": "ckeditor5-collaboration",
  "version": "45.0.0",
  "description": "Base utilities used by CKEditor 5 collaboration features to support multiple users working together in a rich text editor.",
  "license": "SEE LICENSE IN LICENSE.md",
  "author": "CKSource (http://cksource.com/)",
  "homepage": "https://ckeditor.com/collaboration/",
  "keywords": [
    "CKEditor",
    "ckeditor5",
    "ckeditor 5",
    "WYSIWYG",
    "WYSIWYW",
    "text",
    "rich-text",
    "richtext",
    "editor",
    "editing",
    "html",
    "operational transformation",
    "ot",
    "collaboration",
    "collaborative",
    "real-time",
    "realtime",
    "real time",
    "framework"
  ],
  "type": "module",
  "main": "src/index.js",
  "dependencies": {
    "@ckeditor/ckeditor5-collaboration-core": "45.0.0"
  },
  "files": [
    "build",
    "dist",
    "src/**/*.js",
    "src/**/*.d.ts",
    "CHANGELOG.md"
  ],
  "bugs": {
    "url": "https://support.ckeditor.com/hc/en-us/requests/new"
  },
  "types": "src/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js",
      "default": "./src/index.js"
    },
    "./dist/*": {
      "types": "./src/index.d.ts",
      "import": "./dist/*",
      "default": "./dist/*"
    },
    "./src/*": {
      "types": "./src/*.d.ts",
      "import": "./src/*",
      "default": "./src/*"
    },
    "./build/*": "./build/*",
    "./package.json": "./package.json"
  },
  "obfuscated": true
}
